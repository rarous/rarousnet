{%- extends "layouts/common-layout.njk" -%}
{%- set prefix = "og: http://ogp.me/ns# fb: http://ogp.me/ns/fb#" -%}
{%- set images = true -%}
{%- set title= "rarouš.weblog" -%}
{%- set description = "Aleš Roubíček píše o webdesignu a čistém kódu." -%}
{%- set generator = "Gryphoon 3.5 - static site generator" -%}
{%- set bodyId = "rarous" -%}

{% block preload %}
  <link rel="preconnect" href="https://static.cloudflareinsights.com">
  <link rel="preconnect" href="https://challenges.cloudflare.com">
  <link rel="preload" as="font" crossorigin type="font/woff2"
        href="/assets/fonts/iosevka/woff2/iosevka-regular.latin.woff2">
  <link rel="preload" as="font" crossorigin type="font/woff2"
        href="/assets/fonts/iosevka/woff2/iosevka-bold.latin.woff2">
{% endblock %}

{% block links %}
  <link rel="feed" href="/weblog/feed/">
  <link rel="alternate" type="text/mf2+html" href="/weblog/feed/" title="rarouš.weblog - všechny články">
  <link rel="alternate" type="application/rss+xml" href="https://feeds.feedburner.com/rarous-weblog"
        title="rarouš.weblog - poslední články">
  <link rel="alternate" type="application/rss+xml" href="https://feeds.feedburner.com/rarous/weblog/comments"
        title="rarouš.weblog - komentáře k článkům">
  <link rel="alternate" type="application/rss+xml" href="https://feeds.feedburner.com/rarous/w3b"
        title="rarouš.w3b - doporučené čtení">
{% endblock %}

{% block head %}
  <link rel="stylesheet" href="/assets/css/iosevka.css">
  <link rel="stylesheet" href="/assets/css/blog/blog.css">
  <link rel="stylesheet" href="/pagefind/pagefind-ui.css">
  {% block meta %}{% endblock %}
  <meta name="theme-color" content="#f77146">
{% endblock %}

{% block navbar %}
  <li>
    <a
      href="#search"
      title="Hledat na blogu"
      onclick="event.preventDefault();searchDialog.showModal()">
      <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"
           fill="var(--icon-color)">
        <path d="M0 0h24v24H0z" fill="none"/>
        <path
          d="M15.5 14h-.79l-.28-.27C15.41 12.59 16 11.11 16 9.5 16 5.91 13.09 3 9.5 3S3 5.91 3 9.5 5.91 16 9.5 16c1.61 0 3.09-.59 4.23-1.57l.27.28v.79l5 4.99L20.49 19l-4.99-5zm-6 0C7.01 14 5 11.99 5 9.5S7.01 5 9.5 5 14 7.01 14 9.5 11.99 14 9.5 14z"/>
      </svg>
    </a>
  </li>
  <li>
    <a
      href="https://feeds.feedburner.com/rarous-weblog"
      type="application/rss+xml"
      title="Přihlásit se k odběru pomocí RSS">
      <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"
           fill="var(--icon-color)">
        <path d="M0 0h24v24H0z" fill="none"/>
        <circle cx="6.18" cy="17.82" r="2.18"/>
        <path
          d="M4 4.44v2.83c7.03 0 12.73 5.7 12.73 12.73h2.83c0-8.59-6.97-15.56-15.56-15.56zm0 5.66v2.83c3.9 0 7.07 3.17 7.07 7.07h2.83c0-5.47-4.43-9.9-9.9-9.9z"/>
      </svg>
    </a>
  </li>
{% endblock %}

