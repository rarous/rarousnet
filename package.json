{
  "name": "rarousnet",
  "description": "rarous.net website program",
  "main": "index.js",
  "license": "EPL-2.0",
  "dependencies": {
    "@pulumi/aws": "<=1.0",
    "@pulumi/pulumi": "<=1.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.4.9",
    "browser-sync": "^2.26.3",
    "cssnano": "^4.1.10",
    "gulp": "^4.0.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-if": "^2.0.2",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-rev": "^9.0.0",
    "gulp-rev-delete-original": "^0.2.3",
    "gulp-rev-replace": "^0.4.4",
    "gulp-sizereport": "^1.2.1",
    "http-server": "^0.11.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "puppeteer": "1.13"
  },
  "scripts": {
    "prebuild:website": "docker build -t rarousnet ./.circleci/docker/cci-openjdk11-php7-clojure-tools-deps",
    "build:website": "docker run -v `pwd`:/usr/app/src -t rarousnet /bin/sh -c \"cd /usr/app/src/generator && clojure -m rarousnet.generator ../\"",
    "build:assets": "gulp -p",
    "build": "run-s build:*",
    "serve": "http-server ./dist",
    "start": "gulp dev",
    "sync": "aws s3 sync --profile rarous s3://www.rarous.net ./dist\n"
  }
}
